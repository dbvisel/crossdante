{
    "env": {
        "browser": true,
        "commonjs": true
    },
		"parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 0,
				"compat/compat": 2
    },
		"plugins": ["compat"]
}
